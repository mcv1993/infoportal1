<!DOCTYPE html>
<html>
  <head>
    <title>Infoportal1</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
<nav>
  Navigation
  <% if sa_signed_in? %>
    <%= link_to 'Log Out', destroy_sa_session_path, method: :delete %>
  <% end %>

   <% if admin_signed_in? %>
    <%= link_to 'Log Out', destroy_admin_session_path, method: :delete %>
  <% end %>

   <% if tech_signed_in? %>
    <%= link_to 'Log Out', destroy_tech_session_path, method: :delete %>
  <% end %>

</nav>

  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>

    <%= yield %>
  </body>
</html>
